<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>


<GridPane alignment="center" hgap="10" vgap="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <VBox alignment="CENTER" prefHeight="600.0" prefWidth="600.0">
         <children>
            <Pane prefHeight="500.0" prefWidth="500.0">
               <children>
                  <ImageView fx:id="image" fitHeight="500.0" fitWidth="500.0" layoutX="50.0" pickOnBounds="true" preserveRatio="true" />
                  <Canvas fx:id="canvas" height="500.0" layoutX="50.0" width="500.0" />
               </children>
            </Pane>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="start" mnemonicParsing="false" onAction="#clearBoard" text="Start" />
                  <Button fx:id="End" mnemonicParsing="false" onAction="#setEnd" text="End" />
                  <Button mnemonicParsing="false" onAction="#clearBoard" text="Clear" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</GridPane>
